{
  "expo": {
    "name": "Fiddovea",
    "slug": "Fiddovea",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/png/FiddoveaIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/png/firstLogo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "aspectRatio": 1
    },
    "plugins": ["expo-router"],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/png/FiddoveaIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    }
  }
}
